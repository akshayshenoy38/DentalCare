<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akshay.dentalcare3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainPageActivity" />
        <activity
            android:name=".AboutUsActivity"
            android:parentActivityName=".MainPageActivity" />
        <activity
            android:name=".BookingAppointmentActivity"
            android:parentActivityName=".MainPageActivity" />
        <activity
            android:name=".ServicesActivity"
            android:parentActivityName=".MainPageActivity" />
        <activity
            android:name=".AppointmentConfirmActivity"
            android:parentActivityName=".BookingAppointmentActivity" />
        <activity
            android:name=".DocBlogActivity"
            android:parentActivityName=".MainPageActivity" />
        <activity
            android:name=".TestimonialsActivity"
            android:parentActivityName=".MainPageActivity" />
        <activity android:name=".AvailablityActivity"
            android:parentActivityName=".MainPageActivity"></activity>
    </application>

</manifest>